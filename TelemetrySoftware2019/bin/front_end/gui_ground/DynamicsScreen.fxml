<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<GridPane prefHeight="1200.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="front_end.gui_ground.DynamicsScreenController">
  <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" percentWidth="70.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" percentHeight="23.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" percentHeight="23.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" percentHeight="23.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" percentHeight="31.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <LineChart fx:id="rpmChart" animated="false" legendSide="RIGHT" legendVisible="false" GridPane.columnIndex="1">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <LineChart fx:id="speedChart" animated="false" legendSide="RIGHT" legendVisible="false" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <LineChart fx:id="swChart" animated="false" legendSide="RIGHT" legendVisible="false" GridPane.columnIndex="1" GridPane.rowIndex="2">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" styleClass="blue-menu-background" stylesheets="@../../../style/style.css" GridPane.rowSpan="4">
         <children>
            <Label id="nValuesLabel" stylesheets="@../../../style/style.css" text="Pause auto-refresh:" />
            <HBox styleClass="grey-button-box-background" stylesheets="@../../../style/style.css">
               <children>
                  <ToggleButton fx:id="pauseButton" mnemonicParsing="false" onAction="#PressButtonClick" prefWidth="200.0" style="-fx-font-size: 18;" styleClass="transparent-button-background" stylesheets="@../../../style/style.css" text="Pause">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </ToggleButton>
               </children>
               <effect>
                  <DropShadow />
               </effect>
            </HBox>
            <Label id="nValuesLabel" stylesheets="@../../../style/style.css" text="Number of Values:" />
            <ComboBox fx:id="numberValues" prefWidth="200.0" styleClass="combo-box-sidebar" stylesheets="@../../../style/style.css">
               <effect>
                  <DropShadow />
               </effect></ComboBox>
         </children>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <VBox minWidth="180.0" prefWidth="180.0" spacing="5.0" GridPane.columnIndex="2">
         <children>
            <HBox>
               <children>
                  <Label styleClass="chart-labels-bold" stylesheets="@../../../style/style.css" text="RPM:">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="rpmLabel" styleClass="chart-labels" stylesheets="@../../../style/style.css" text="No Value" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
      <VBox minWidth="180.0" prefWidth="180.0" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
         <children>
            <HBox>
               <children>
                  <Label styleClass="chart-labels-bold" stylesheets="@../../../style/style.css" text="Speed:">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="speedLabel" styleClass="chart-labels" stylesheets="@../../../style/style.css" text="No Value" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
      <VBox minWidth="180.0" prefWidth="180.0" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="2">
         <children>
            <HBox>
               <children>
                  <Label styleClass="chart-labels-bold" stylesheets="@../../../style/style.css" text="SW Angle:">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="swLabel" styleClass="chart-labels" stylesheets="@../../../style/style.css" text="No Value" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
      <VBox minWidth="180.0" prefWidth="180.0" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="3">
         <children>
            <HBox>
               <children>
                  <Label styleClass="chart-labels-bold" stylesheets="@../../../style/style.css" text="Gear:">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="gearLabel" styleClass="chart-labels" stylesheets="@../../../style/style.css" text="No Value" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <children>
            <LineChart fx:id="gearChart" animated="false" legendSide="RIGHT" legendVisible="false">
               <xAxis>
                  <CategoryAxis side="BOTTOM" />
               </xAxis>
               <yAxis>
                  <NumberAxis side="LEFT" />
               </yAxis>
            </LineChart>
            <Slider fx:id="slider">
               <VBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" />
               </VBox.margin>
            </Slider>
         </children>
      </VBox>
   </children>
</GridPane>
